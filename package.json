    "flow-bin": "^0.83.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "i18next": "^11.9.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-emotion": "^9.2.11",
    "json-server": "^0.14.0",
    "lint-staged": "^7.3.0",
    "moment": "^2.22.2",
    "opn": "^5.4.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
