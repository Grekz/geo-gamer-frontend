{
  "name": "webpack-grekz",
  "version": "1.0.1",
  "private": true,
  "description": "Webpack + React Base application",
  "main": "index.js",
  "scripts": {
    "mockServer": "node json-server/server.js",
    "test": "flow && jest",
    "test:live": "jest --watch",
    "start": "webpack-dev-server --mode development --hot --open --port 3000 --history-api-fallback",
    "startWithServer": "concurrently 'npm run mockServer' 'npm start' ",
    "build": "webpack --mode production",
    "lint": "eslint src",
    "lint-fix": "eslint src --ext .js,.jsx --fix",
    "lint-staged": "flow && lint-staged",
    "flow": "flow",
    "jest": "jest",
    "jest:full": "jest",
    "pull": "git pull && npm prune && npm install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.{jsx,js}": [
      "prettier --write",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "keywords": [
    "react",
    "jest"
  ],
  "author": "Grekz",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
